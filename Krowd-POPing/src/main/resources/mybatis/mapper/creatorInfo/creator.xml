<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jellybears.krowdpoping.creatorinfomation.model.dao.CreatorMapper">

    <select id="selectCreator" resultType="com.jellybears.krowdpoping.creatorinfomation.model.dto.CreatorDTO">
        select *
        from creator
    </select>

    <insert id="insertCreator" parameterType="com.jellybears.krowdpoping.creatorinfomation.model.dto.CreatorDTO">
        insert into creator (
            CR_TYPE,
            CR_BUSINESS_NUM,
            PHONE,
            EMAIL,
            ADDRESS,
            INTRODUCE
        )
        values (
                #{crType},
                #{crBusinessNum},
                #{phone},
                #{email},
                #{address},
                #{introduce}
               )
    </insert>

</mapper>

